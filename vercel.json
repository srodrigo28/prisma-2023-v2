{
    "version": 2,
    "functions": {
        "src/server.ts": {
          "memory": 3008,
          "maxDuration": 60
        }
      },
    "builds": [
        {
            "src": "/src/server.ts",
            "use": "@vercel/node" 
        }
    ],
    "routes": [
        {
            "src": "(.*)",
            "dest": "/src/server.ts"
        }
    ]
}